{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "Context/CartContext.tsx", "components/Cart/Cart.tsx", "app/cart/page.tsx", "components/ProductsCards/ProductsCards.tsx", "app/products/[id]/page.tsx", "db/product.js", "app/searchpage/[term]/page.jsx"],
  "exclude": ["node_modules"]
}
